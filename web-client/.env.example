# Hospital Management System - Environment Variables
# Copy this file to .env and update values as needed

# API Configuration
VITE_API_URL=http://localhost:8080

# Hospital Configuration (for white-label deployments)
VITE_HOSPITAL_NAME=Rainbow Health Clinic
VITE_HOSPITAL_TAGLINE=Your Health, Our Priority

# Feature Flags
VITE_ENABLE_TELECONSULTATION=true
VITE_ENABLE_ONLINE_PAYMENT=true
VITE_ENABLE_LAB_BOOKING=true

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=
