{
  "name": "hms-web-client",
  "version": "1.0.0",
  "description": "Hospital Management System - Web Client",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
